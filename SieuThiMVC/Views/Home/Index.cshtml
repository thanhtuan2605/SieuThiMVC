
@{
    ViewBag.Title = "Home Page";
}

@section featured {
    
    <div>
        @if(ViewBag.ListAds!=0)
        {
            <ul>
            @for(var i=0; i<ViewBag.ListAds; i++)
            {
                <li><img src=@ViewBag.ListAds[i].ImgLink /></li>
            }
            </ul>
        }
        
    </div>
}

